{
  "name": "tile-server",
  "version": "1.0.2",
  "description": "Tile server",
  "account": "quanghuy2307",
  "main": "src/main.js",
  "bin": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "build-image": "ACCOUNT=$(grep -oP '(?<=\"account\": \")[^\"]*' package.json) && NAME=$(grep -oP '(?<=\"name\": \")[^\"]*' package.json) && TAG=$(grep -oP '(?<=\"version\": \")[^\"]*' package.json) && docker build -t $ACCOUNT/$NAME:$TAG .",
    "release-image": "ACCOUNT=$(grep -oP '(?<=\"account\": \")[^\"]*' package.json) && NAME=$(grep -oP '(?<=\"name\": \")[^\"]*' package.json) && TAG=$(grep -oP '(?<=\"version\": \")[^\"]*' package.json) && docker push $ACCOUNT/$NAME:$TAG"
  },
  "dependencies": {
    "@mapbox/glyph-pbf-composite": "0.0.3",
    "@mapbox/mbtiles": "0.12.1",
    "@mapbox/sphericalmercator": "1.2.0",
    "@mapbox/vector-tile": "1.3.1",
    "@maplibre/maplibre-gl-native": "5.4.0",
    "@maplibre/maplibre-gl-style-spec": "20.3.0",
    "advanced-pool": "0.3.3",
    "axios": "1.7.2",
    "chokidar": "3.6.0",
    "color": "4.2.3",
    "commander": "12.1.0",
    "express": "4.19.2",
    "handlebars": "4.7.8",
    "morgan": "1.10.0",
    "pbf": "3.2.1",
    "pmtiles": "3.0.6",
    "png-validator": "2.0.0",
    "proj4": "2.11.0",
    "sharp": "0.33.4"
  },
  "devDependencies": {
    "@types/color": "3.0.6",
    "@types/express": "4.17.21",
    "@types/mapbox__sphericalmercator": "1.2.3",
    "@types/mapbox__vector-tile": "1.3.4",
    "@types/morgan": "1.9.9",
    "@types/pbf": "3.0.5",
    "@types/proj4": "2.5.5",
    "nodemon": "3.1.3",
    "prettier": "3.3.2"
  },
  "keywords": [
    "maplibre-gl",
    "tile-server"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=19 <21"
  }
}
