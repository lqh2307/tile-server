{
  "name": "tile-server",
  "version": "1.0.0",
  "description": "Map tile server",
  "account": "quanghuy2307",
  "main": "src/main.js",
  "bin": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "build-image": "ACCOUNT=$(grep -oP '(?<=\"account\": \")[^\"]*' package.json) && NAME=$(grep -oP '(?<=\"name\": \")[^\"]*' package.json) && TAG=$(grep -oP '(?<=\"version\": \")[^\"]*' package.json) && docker build -t $ACCOUNT/$NAME:$TAG ."
  },
  "dependencies": {
    "@mapbox/glyph-pbf-composite": "^0.0.3",
    "@mapbox/mbtiles": "0.12.1",
    "@mapbox/polyline": "^1.2.1",
    "@mapbox/sphericalmercator": "1.2.0",
    "@mapbox/vector-tile": "1.3.1",
    "@maplibre/maplibre-gl-native": "5.4.0",
    "@maplibre/maplibre-gl-style-spec": "20.1.1",
    "@sindresorhus/fnv1a": "3.1.0",
    "advanced-pool": "0.3.3",
    "axios": "^1.6.8",
    "canvas": "2.11.2",
    "chokidar": "3.6.0",
    "clone": "2.1.2",
    "color": "4.2.3",
    "commander": "^12.0.0",
    "express": "4.19.2",
    "handlebars": "4.7.8",
    "http-shutdown": "1.2.2",
    "morgan": "1.10.0",
    "pbf": "3.2.1",
    "pmtiles": "3.0.5",
    "proj4": "2.11.0",
    "sanitize-filename": "1.6.3",
    "sharp": "0.33.3"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  },
  "keywords": [
    "maplibre-gl",
    "tile-server"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=18.17.0 <21"
  }
}
