{
  "options": {
    "listenPort": 8080,
    "serverEndpoint": true,
    "configEndpoint": true,
    "serveFrontPage": true,
    "serveSwagger": true,
    "serveRendered": true,
    "maxScaleRender": 2,
    "renderedCompression": 1,
    "minRenderedPoolSize": 8,
    "maxRenderedPoolSize": 16,
    "loggerFormat": ":date[iso] [INFO] :method :url :status :res[content-length] :response-time :remote-addr :user-agent",
    "taskSchedule": "0 0 0 * * *",
    "restartServerAfterTask": true,
    "process": 1
  },
  "styles": {},
  "datas": {},
  "sprites": {},
  "fonts": {}
}
